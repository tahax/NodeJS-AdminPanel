<nav>
  <ul class="pagination justify-content-center ws-dr-ltr">
    <% if(currentPage !== 1 && previousPage !== 1) {%>
    <li class="page-item"><a class="page-link" href="?page=1">1</a></li>
    <% } %> <%if(previous3Page > 1) { %>
    <li class="page-item">
      <a class="page-link" href="?page=<%= previous3Page %>"
        ><%= previous3Page %></a
      >
    </li>
    <% } %> <%if(previous2Page > 1) { %>
    <li class="page-item">
      <a class="page-link" href="?page=<%= previous2Page %>"
        ><%= previous2Page %></a
      >
    </li>
    <% } %> <%if(hasPreviousPage) { %>
    <li class="page-item">
      <a class="page-link" href="?page=<%= previousPage %>"
        ><%= previousPage %></a
      >
    </li>
    <% } %>
    <li class="page-item active">
      <a class="page-link" href="?page=<%= currentPage %>"
        ><%= currentPage %></a
      >
    </li>
    <% if(hasNextPage) {%>
    <li class="page-item">
      <a class="page-link" href="?page=<%= nextPage %>"><%= nextPage %></a>
    </li>
    <% } %> <% if(next2Page < lastPage ) {%>
    <li class="page-item">
      <a class="page-link" href="?page=<%= next2Page %>"><%= next2Page %></a>
    </li>
    <% } %> <% if(next3Page < lastPage ) {%>
    <li class="page-item">
      <a class="page-link" href="?page=<%= next3Page %>"><%= next3Page %></a>
    </li>
    <% } %>
    <li class="page-item"><span class="page-link">...</span></li>
    <% if(lastPage !== currentPage && nextPage !== lastPage) {%>
    <li class="page-item">
      <a class="page-link" href="?page=<%= lastPage %>"><%= lastPage %></a>
    </li>
    <% } %>
  </ul>
</nav>
